CHIP Add5 {
    IN a[5], b=[5], c;
    OUT sum[5], carry[5];

    PARTS:
    FullAdder (a=a[4], b=b[4], c=c,
        sum=sum[4], carry=carry[4]);

    FullAdder (a=a[3], b=b[3], c=carry[4],
        sum=carry[3], carry=carry[3]);

    FullAdder (a=a[2], b=b[2], c=carry[3],
        sum=sum[2], carry=carry[2]);

    FullAdder (a=a[1], b=b[1], c=carry[2],
        sum=sum[1], carry=carry[1]);

    FullAdder (a=a[0], b=b[0], c=carry[1],
        sum=sum[0], carry=carry[0]);
}
